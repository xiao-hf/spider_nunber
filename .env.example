# Base URLs
BASE_URL=https://www.opene164.org.cn
INDEX_URL=https://www.opene164.org.cn/mark/index.html
CAPTCHA_URL=https://www.opene164.org.cn/captcha.html
QUERY_URL=https://www.opene164.org.cn/mark/data.do

# HTTP
REQUEST_TIMEOUT=15
VERIFY_SSL=true
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36
REFERER=https://opene164.org.cn/mark/index.html
ORIGIN=https://opene164.org.cn

# Query payload
QUERY_METHOD=POST
QUERY_CONTENT_TYPE=form
CODE_FIELD=code
CAPTCHA_FIELD=captcha
EXTRA_FORM_JSON={}
EXTRA_HEADERS_JSON={"X-Requested-With":"XMLHttpRequest"}

# Captcha
CAPTCHA_LEN=4
CAPTCHA_CASE=lower
CAPTCHA_REGEX=
CAPTCHA_ERROR_HINTS=
CAPTCHA_MAX_TRIES=3
CAPTCHA_ERROR_HINT=
CAPTCHA_REFRESH_PARAM=true
CAPTCHA_REFRESH_PARAM_NAME=t
SAVE_CAPTCHA=false

# OCR
OCR_WHITELIST=abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789
OCR_THRESHOLD=140

# Proxy
PROXY_MODE=none
PROXY_URL=
PROXY_API_URL=
PROXY_API_ACTIVE_URL=
PROXY_API_RELEASE_URL=
PROXY_API_HEADERS_JSON={}
PROXY_API_PARAMS_JSON={}
PROXY_API_TIMEOUT=10
PROXY_SCHEME=http
PROXY_USERNAME=
PROXY_PASSWORD=
PROXY_ALWAYS_REFRESH=false
PROXY_ROTATE_EACH_REQUEST=false
PROXY_REFRESH_BEFORE_SECONDS=5
PROXY_ROTATE_ON_LIMIT=false
PROXY_LIMIT_HINT=当天查询次数已达上限
PROXY_LIMIT_HINTS=
PROXY_LIMIT_STATUSES=
PROXY_RELEASE_ON_LIMIT=false
PROXY_DEBUG_IP_CHECK=false
PROXY_DEBUG_IP_URL=https://api.ipify.org
PROXY_DEBUG_IP_TIMEOUT=5
